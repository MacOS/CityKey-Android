<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginStart="50dp"
    android:layout_marginTop="10dp"
    android:layout_marginEnd="50dp">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numOne"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="1"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numFour"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numTwo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_max="55dp" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numTwo"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="2"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numFive"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numThree"
        app:layout_constraintStart_toEndOf="@id/numOne"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numThree"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="3"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numSix"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/numTwo"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numFour"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="4"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numSeven"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numFive"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numOne" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numFive"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="5"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numEight"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numSix"
        app:layout_constraintStart_toEndOf="@id/numFour"
        app:layout_constraintTop_toBottomOf="@id/numTwo" />


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numSix"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="6"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numNine"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/numFive"
        app:layout_constraintTop_toBottomOf="@id/numThree" />


    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numSeven"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="7"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numTogglePass"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numEight"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numFour" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numEight"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="8"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numZero"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numNine"
        app:layout_constraintStart_toEndOf="@id/numSeven"
        app:layout_constraintTop_toBottomOf="@id/numFive" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numNine"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="9"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@id/numDelete"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/numEight"
        app:layout_constraintTop_toBottomOf="@id/numSix" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/numTogglePass"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:contentDescription="@string/egov_accessibility_text_eye_button"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numZero"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/numSeven"
        app:srcCompat="@drawable/ic_auth_input_toggle_password_off" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/numZero"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:foreground="@drawable/circular_background"
        android:gravity="center"
        android:text="0"
        android:textColor="@color/onSurface"
        app:autoSizeMaxTextSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toStartOf="@id/numDelete"
        app:layout_constraintStart_toEndOf="@id/numTogglePass"
        app:layout_constraintTop_toBottomOf="@id/numEight" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/numDelete"
        style="@style/RobotoRegular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="?selectableItemBackgroundBorderless"
        android:contentDescription="@string/egov_accessibility_text_clear_button"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H, 1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/numZero"
        app:layout_constraintTop_toBottomOf="@id/numNine"
        app:srcCompat="@drawable/ic_auth_input_delete" />

</androidx.constraintlayout.widget.ConstraintLayout>